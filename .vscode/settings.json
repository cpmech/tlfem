{
  "files.exclude": {
    "**/__pycache__/": true,
    "**/*.py[cod]": true,
    "**/*.so": true,
    "**/.Python": true,
    "**/env/": true,
    "**/build/": true,
    "**/develop-eggs/": true,
    "**/dist/": true,
    "**/downloads/": true,
    "**/eggs/": true,
    "**/.eggs/": true,
    "**/lib/": true,
    "**/lib64/": true,
    "**/parts/": true,
    "**/sdist/": true,
    "**/var/": true,
    "**/*.egg-info/": true,
    "**/.installed.cfg": true,
    "**/*.egg": true,
    "**/*.manifest": true,
    "**/*.spec": true,
    "**/pip-log.txt": true,
    "**/pip-delete-this-directory.txt": true,
    "**/htmlcov/": true,
    "**/.tox/": true,
    "**/.coverage": true,
    "**/.coverage.*": true,
    "**/.cache": true,
    "**/nosetests.xml": true,
    "**/coverage.xml": true,
    "**/*,cover": true,
    "**/*.mo": true,
    "**/*.pot": true,
    "**/*.log": true,
    "**/docs/_build/": true,
    "**/target/": true,
    "**/.vscode": true
  },
  "python.formatting.provider": "black",
  "cSpell.words": [
    "tlfem"
  ]
}